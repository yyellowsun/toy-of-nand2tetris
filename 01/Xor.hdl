/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a,out=v1);
    Not(in=b,out=v2);
    And(a=v1,b=b,out=v3);
    And(a=a,b=v2,out=v4);
    Or(a=v3,b=v4,out=out);
}